# 代理提供者配置
proxy-providers:
  机场:
    type: http
    url: https://huaikhwang.central-world.org/api/v1/trails/bolster?token=bc1e37a26d87fe87242cc3925ae0e9b4
    path: ./proxy_provider/jc.yaml
    udp: false
    health-check:
      enable: true
      interval: 600               # 健康检查间隔(秒)
      url: http://www.gstatic.com/generate_204

# 代理组配置
proxy-groups:
  # 主要代理选择组
  - name: 主线路
    type: select
    proxies:
      - DIRECT                    # 直连
      - 谷歌分流                  # Google服务代理组
    use:
      - 机场                      # 使用机场订阅
    disable-udp: true
    health-check:
      enable: true
      interval: 600
      url: http://www.gstatic.com/generate_204

  # Google服务代理组
  - name: 谷歌分流
    type: select
    proxies:
      - Play                      # Play商店专用代理组
      - REJECT                    # 拒绝连接
    disable-udp: false
    health-check:
      enable: false
      interval: 600
      url: http://www.gstatic.com/generate_204

  # Play商店专用代理组
  - name: Play
    type: select
    proxies:
      - 缅甸1
      - 缅甸2
      - 缅甸3
      - 缅甸4
      - 缅甸5
      - 缅甸6
      - 缅甸7
      - 缅甸8
      - 缅甸9
      - 缅甸10
      
    disable-udp: false
    health-check:
      enable: false
      interval: 600
      url: http://www.gstatic.com/generate_204

# 分流规则
rules:
  # Google Play相关服务
  - DOMAIN,play.googleapis.com,谷歌分流
  - DOMAIN,myaccount.google.com,谷歌分流
  - DOMAIN-KEYWORD,accounts.google.com,谷歌分流
  - DOMAIN,play-fe.googleapis.com,谷歌分流
  - DOMAIN,sspanel.net,谷歌分流
  - DOMAIN,payments-pa.googleapis.com,谷歌分流
    # === IP 检测网站 ===
  - DOMAIN-SUFFIX,ip.sb,缅甸线路                # IP.SB查询服务
  - DOMAIN-SUFFIX,ipip.net,缅甸线路             # IPIP.NET IP数据库
  - DOMAIN-SUFFIX,ip138.com,缅甸线路            # IP138查询网站
  - DOMAIN,ip.chinaz.com,缅甸线路               # 站长之家IP查询
  - DOMAIN,whatismyip.com,缅甸线路              # What Is My IP查询
  - DOMAIN,api.ip.sb,缅甸线路                   # IP.SB API服务
  - DOMAIN-SUFFIX,whatismyaddress.app,缅甸线路  # IP地址查询应用
  - DOMAIN,whatismyaddress.app,缅甸线路         # IP地址查询应用
  - DOMAIN-SUFFIX,ip2location.com,缅甸线路      # IP2Location数据库
  - DOMAIN,ip2location.com,缅甸线路             # IP2Location查询
  - DOMAIN-SUFFIX,ipapi.co,缅甸线路             # IP API查询服务
  - DOMAIN-SUFFIX,ip-api.com,缅甸线路           # IP-API地理位置查询
  - DOMAIN-SUFFIX,ipleak.net,缅甸线路           # IP泄露检测
  - DOMAIN-SUFFIX,browserleaks.com,缅甸线路     # 浏览器信息泄露检测

  # 未匹配流量默认使用主线路
  - MATCH,主线路
